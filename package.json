  },
  "homepage": "https://github.com/onetag-dev/nodejs-test#readme",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.21",
    "uuid": "^9.0.0"
  }
}
